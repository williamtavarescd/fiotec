 key: 'calories', order: 'asc' }]\"\n      items-per-page=\"5\"\n      style=\"max-width: 600px;\"\n    >\n      <template v-slot:top>\n        <v-toolbar\n          flat\n        >\n          <v-toolbar-title>Alunos</v-toolbar-title>\n          <v-divider\n            class=\"mx-4\"\n            inset\n            vertical\n          />\n          <v-spacer></v-spacer>\n          <NewStudent\n            @reload=\"listStudents\"\n          />\n          <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\n            <v-card>\n              <v-card-title class=\"text-h5\">Are you sure you want to delete this item?</v-card-title>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn color=\"blue-darken-1\" variant=\"text\" @click=\"closeDelete\">Cancel</v-btn>\n                <v-btn color=\"blue-darken-1\" variant=\"text\" @click=\"deleteStudentConfirm\">OK</v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n        </v-toolbar>\n      </template>\n      <template v-slot:item.actions=\"{ item }\">\n        <v-icon\n          class=\"me-2\"\n          size=\"small\"\n          @click=\"editItem(item)\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-4\"\n          size=\"small\"\n          @click=\"deleteStudent(item)\"\n        >\n          mdi-delete\n        </v-icon>\n      </template>\n      <template v-slot:no-data>\n        <v-progress-linear\n          color=\"#008ABD\"\n          indeterminate\n        />\n        <br>\n        Carregando...\n      </template>\n    </v-data-table>\n</div>\n</template>\n<script>\n  import api from '@/api/api'\n  import NewStudent from '@/components/students/NewStudents.vue'\n    export default {\n    name: 'Students',\n\n    components: {\n      NewStudent\n    },\n\n    layout: 'Default',\n      data: () => ({\n        dialog: false,\n        dialogDelete: false,\n        selectedStudent: null,\n        headers: [\n          {\n            title: 'ID',\n            align: 'center',\n            key: 'id',\n            sortable: false\n          },\n          {\n            title: 'Nome',\n            align: 'center',\n            key: 'name',\n            sortable: false\n          },\n\n        { title: '',\n          key: 'actions', \n          align: 'end',\n          sortable: false\n        },\n        ],\n        students: [],\n        editedIndex: -1\n      }),\n\n      computed: {\n      },\n\n      watch: {\n      },\n\n      async created () {\n        await this.listStudents()\n      },\n\n      methods: {\n        editItem (item) {\n          console.log(item)\n        },\n\n        deleteStudent (student) {\n          this.selectedStudent = student\n          this.dialogDelete = true\n        },\n\n        async deleteStudentConfirm () {\n          this.loading = true\n          try {\n            await api.deleteStudent(this.selectedStudent.id)\n            this.loading = false\n          } catch (e) {\n            this.loading = false\n            console.log(e)\n          }\n        },\n\n        async listStudents () {\n          this.loading = true\n          try {\n            this.students = await api.listStudents()\n            this.loading = false\n          } catch (e) {\n            this.loading = false\n            console.log(e)\n          }\n        }\n      }\n  }\n</script>"],"mappings":"AAgEE,OAAOA,GAAE,MAAO,WAAU;AAC1B,OAAOC,UAAS,MAAO,uCAAsC;AAC3D,eAAe;EACfC,IAAI,EAAE,UAAU;EAEhBC,UAAU,EAAE;IACVF;EACF,CAAC;EAEDG,MAAM,EAAE,SAAS;EACfC,IAAI,EAAEA,CAAA,MAAO;IACXC,MAAM,EAAE,KAAK;IACbC,YAAY,EAAE,KAAK;IACnBC,eAAe,EAAE,IAAI;IACrBC,OAAO,EAAE,CACP;MACEC,KAAK,EAAE,IAAI;MACXC,KAAK,EAAE,QAAQ;MACfC,GAAG,EAAE,IAAI;MACTC,QAAQ,EAAE;IACZ,CAAC,EACD;MACEH,KAAK,EAAE,MAAM;MACbC,KAAK,EAAE,QAAQ;MACfC,GAAG,EAAE,MAAM;MACXC,QAAQ,EAAE;IACZ,CAAC,EAEH;MAAEH,KAAK,EAAE,EAAE;MACTE,GAAG,EAAE,SAAS;MACdD,KAAK,EAAE,KAAK;MACZE,QAAQ,E
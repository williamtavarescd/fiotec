{"ast":null,"code":"import api from '@/api/api';\nimport AddStudent from '@/components/schoolCourses/AddStudent.vue';\nimport AddSubject from '@/components/schoolCourses/AddSubject.vue';\nimport EditSchoolCourse from '@/components/schoolCourses/EditSchoolCourse.vue';\nimport { useSnackBar } from '@/stores/snackBar.js';\nexport default {\n  name: 'EditSchoolCourses',\n  components: {\n    AddStudent,\n    AddSubject,\n    EditSchoolCourse\n  },\n  layout: 'Default',\n  data: () => ({\n    dialog: false,\n    loading: false,\n    dialogEditSchoolCourse: false,\n    schoolCourseName: '',\n    schoolCourse: {}\n  }),\n  computed: {\n    schoolCourseId() {\n      return parseInt(this.$route.params.id);\n    },\n    snackBar() {\n      return useSnackBar();\n    }\n  },\n  async created() {\n    await this.getSchoolCourse();\n  },\n  methods: {\n    editSchoolCourse() {\n      this.schoolCourseName = this.schoolCourse.name;\n      this.dialogEditSchoolCourse = true;\n    },\n    closeDialogs() {\n      this.dialogEditSchoolCourse = false;\n      this.schoolCourseName = '';\n    },\n    async deleteStudent(student) {\n      this.loading = true;\n      try {\n        await api.unenrollment(student.pivot.id);\n        await this.getSchoolCourse();\n        this.snackBar.increment({\n          show: true,\n          msg: 'Aluno retirado da turma com sucesso',\n          color: 'success'\n        });\n        this.loading = false;\n      } catch (e) {\n        this.loading = false;\n        console.log(e);\n      }\n    },\n    async deleteSchoolCourseSubject(subject) {\n      this.loading = true;\n      try {\n        await api.deleteSchoolCourseSubject(subject.pivot.id);\n        await this.getSchoolCourse();\n        this.snackBar.increment({\n          show: true,\n          msg: 'Matéria retirada da turma com sucesso',\n          color: 'success'\n        });\n        this.loading = false;\n      } catch (e) {\n        this.loading = false;\n        console.log(e);\n      }\n    },\n    async getSchoolCourse() {\n      this.loading = true;\n      try {\n        const request = await api.getSchoolCourse(this.schoolCourseId);\n        this.schoolCourse = request;\n        this.loading = false;\n      } catch (e) {\n        this.loading = false;\n        console.log(e);\n      }\n    }\n  }\n};","map":{"version":3,"names":["api","AddStudent","AddSubject","EditSchoolCourse","useSnackBar","name","components","layout","data","dialog","loading","dialogEditSchoolCourse","schoolCourseName","schoolCourse","computed","schoolCourseId","parseInt","$route","params","id","snackBar","created","getSchoolCourse","methods","editSchoolCourse","closeDialogs","deleteStudent","student","unenrollment","pivot","increment","show","msg","color","e","console","log","deleteSchoolCourseSubject","subject","request"],"sources":["/app/src/views/EditSchoolCourse.vue"],"sourcesContent":["<template>\n  <div class=\"d-flex justify-center\">\n    <v-card\n      color=\"grey-lighten-4\"\n      height=\"600px\"\n      width=\"600px\"\n      rounded=\"0\"\n      flat\n    >\n      <v-toolbar density=\"compact\">\n        <v-icon class=\"ml-2\" @click=\"$router.go( -1 )\">mdi-arrow-left-bold</v-icon>\n        <v-skeleton-loader\n          v-if=\"loading\"\n          width=\"400\"\n          color=\"transparent\"\n          type=\"list-item@1\"\n        />\n        <v-toolbar-title v-else><strong> {{ schoolCourse?.name  }} </strong></v-toolbar-title>\n\n        <v-spacer></v-spacer>\n\n        <v-btn icon \n          @click=\"editSchoolCourse(item)\"\n          :disabled=\"loading\"\n        >\n          <v-icon>mdi-pencil</v-icon>\n        </v-btn>\n      </v-toolbar>\n\n      <div class=\"d-flex flex-row\">\n        <v-list lines=\"students\" class=\"flex-grow-1\">\n            <div class=\"d-flex justify-space-around align-center mt-2\">\n              <h3>Alunos</h3>\n              <AddStudent\n                :school-course-id=\"schoolCourseId\"\n                :load=\"loading\"\n                @reload=\"getSchoolCourse\"\n              />\n            </div>\n            <div class=\"align-start mt-2\">\n              <v-skeleton-loader\n                v-if=\"loading\"\n                type=\"list-item@3\"\n              />\n              <v-list-item\n                v-else\n                v-for=\"student in schoolCourse.students\"\n                :key=\"student.id\"\n              >\n              <v-list-item-title><strong> {{ student.name }} </strong></v-list-item-title>\n                <template v-slot:append>\n                  <v-btn\n                    icon=\"mdi-account-minus\"\n                    size=\"small\"\n                    variant=\"text\"\n                    :disabled=\"loading\"\n                    @click=\"deleteStudent(student)\"\n                  />\n                </template>\n              </v-list-item>\n          </div>\n        </v-list>\n\n        <v-list lines=\"subjects\" class=\"flex-grow-1\">\n            <div class=\"d-flex justify-space-around align-center mt-2\">\n              <h3>Matérias</h3>\n              <AddSubject\n                :school-course-id=\"schoolCourseId\"\n                :load=\"loading\"\n                @reload=\"getSchoolCourse\"\n              />\n            </div>\n            <div class=\"align-start mt-2\">\n              <v-skeleton-loader\n                v-if=\"loading\"\n                type=\"list-item@3\"\n              />\n              <v-list-item\n                v-else\n                v-for=\"subject in schoolCourse.subjects\"\n                :key=\"subject.id\"\n                start\n              >\n              <v-list-item-title><strong> {{ subject.name }} </strong></v-list-item-title>\n                <template v-slot:append>\n                  <v-btn\n                    icon=\"mdi-file-minus\"\n                    size=\"small\"\n                    variant=\"text\"\n                    :disabled=\"loading\"\n                    @click=\"deleteSchoolCourseSubject(subject)\"\n                  />\n                </template>\n              </v-list-item>\n          </div>\n        </v-list>\n      </div>\n    </v-card>\n\n    <EditSchoolCourse\n      v-model=\"dialogEditSchoolCourse\"\n      :school-course-id=\"schoolCourseId\"\n      :school-course-name=\"schoolCourseName\"\n      @reload=\"getSchoolCourse\"\n      @close=\"closeDialogs\"\n    />\n  </div>\n</template>\n<script>\n  import api from '@/api/api'\n  import AddStudent from '@/components/schoolCourses/AddStudent.vue'\n  import AddSubject from '@/components/schoolCourses/AddSubject.vue'\n  import EditSchoolCourse from '@/components/schoolCourses/EditSchoolCourse.vue'\n  import { useSnackBar } from '@/stores/snackBar.js'\n\n    export default {\n    name: 'EditSchoolCourses',\n\n    components: {\n      AddStudent,\n      AddSubject,\n      EditSchoolCourse\n    },\n\n    layout: 'Default',\n      data: () => ({\n        dialog: false,\n        loading: false,\n        dialogEditSchoolCourse: false,\n        schoolCourseName: '',\n        schoolCourse: {}\n      }),\n      computed: {\n        schoolCourseId () {\n          return parseInt(this.$route.params.id)\n        },\n        snackBar() {\n          return useSnackBar()\n        }\n      },\n\n      async created () {\n        await this.getSchoolCourse()\n      },\n\n      methods: {\n        editSchoolCourse () {\n          this.schoolCourseName = this.schoolCourse.name\n          this.dialogEditSchoolCourse = true\n        },\n        closeDialogs () {\n          this.dialogEditSchoolCourse = false\n          this.schoolCourseName = ''\n        },\n        async deleteStudent(student) {\n          this.loading = true\n          try {\n            await api.unenrollment(student.pivot.id)\n            await this.getSchoolCourse()\n            this.snackBar.increment({show: true, msg: 'Aluno retirado da turma com sucesso', color: 'success'} )\n            this.loading = false\n          } catch (e) {\n            this.loading = false\n            console.log(e)\n          }\n        },\n\n        async deleteSchoolCourseSubject(subject) {\n          this.loading = true\n          try {\n            await api.deleteSchoolCourseSubject(subject.pivot.id)\n            await this.getSchoolCourse()\n            this.snackBar.increment({show: true, msg: 'Matéria retirada da turma com sucesso', color: 'success'} )\n            this.loading = false\n          } catch (e) {\n            this.loading = false\n            console.log(e)\n          }\n        },\n\n        async getSchoolCourse () {\n          this.loading = true\n          try {\n            const request = await api.getSchoolCourse(this.schoolCourseId)\n            this.schoolCourse = request\n            this.loading = false\n          } catch (e) {\n            this.loading = false\n            console.log(e)\n          }\n        }\n      }\n  }\n</script>"],"mappings":"AA6GE,OAAOA,GAAE,MAAO,WAAU;AAC1B,OAAOC,UAAS,MAAO,2CAA0C;AACjE,OAAOC,UAAS,MAAO,2CAA0C;AACjE,OAAOC,gBAAe,MAAO,iDAAgD;AAC7E,SAASC,WAAU,QAAS,sBAAqB;AAE/C,eAAe;EACfC,IAAI,EAAE,mBAAmB;EAEzBC,UAAU,EAAE;IACVL,UAAU;IACVC,UAAU;IACVC;EACF,CAAC;EAEDI,MAAM,EAAE,SAAS;EACfC,IAAI,EAAEA,CAAA,MAAO;IACXC,MAAM,EAAE,KAAK;IACbC,OAAO,EAAE,KAAK;IACdC,sBAAsB,EAAE,KAAK;IAC7BC,gBAAgB,EAAE,EAAE;IACpBC,YAAY,EAAE,CAAC;EACjB,CAAC,CAAC;EACFC,QAAQ,EAAE;IACRC,cAAaA,CAAA,EAAK;MAChB,OAAOC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;IACvC,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,OAAOhB,WAAW,CAAC;IACrB;EACF,CAAC;EAED,MAAMiB,OAAMA,CAAA,EAAK;IACf,MAAM,IAAI,CAACC,eAAe,CAAC;EAC7B,CAAC;EAEDC,OAAO,EAAE;IACPC,gBAAeA,CAAA,EAAK;MAClB,IAAI,CAACZ,gBAAe,GAAI,IAAI,CAACC,YAAY,CAACR,IAAG;MAC7C,IAAI,CAACM,sBAAqB,GAAI,IAAG;IACnC,CAAC;IACDc,YAAWA,CAAA,EAAK;MACd,IAAI,CAACd,sBAAqB,GAAI,KAAI;MAClC,IAAI,CAACC,gBAAe,GAAI,EAAC;IAC3B,CAAC;IACD,MAAMc,aAAaA,CAACC,OAAO,EAAE;MAC3B,IAAI,CAACjB,OAAM,GAAI,IAAG;MAClB,IAAI;QACF,MAAMV,GAAG,CAAC4B,YAAY,CAACD,OAAO,CAACE,KAAK,CAACV,EAAE;QACvC
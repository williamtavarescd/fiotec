  v-for=\"subject in schoolCourse?.subjects\"\n            :key=\"subject.id\"\n            :title=\"subject.name\"\n          >\n            <template v-slot:prepend>\n              <v-avatar color=\"grey-lighten-1\">\n                <v-icon color=\"white\">mdi-folder</v-icon>\n              </v-avatar>\n            </template>\n\n            <template v-slot:append>\n              <v-btn\n                color=\"grey-lighten-1\"\n                icon=\"mdi-information\"\n                variant=\"text\"\n              ></v-btn>\n            </template>\n          </v-list-item>\n        </v-list>\n      </div>\n    </v-card>\n\n    <EditSchoolCourse\n      v-model=\"dialogEditSchoolCourse\"\n      :school-course-id=\"schoolCourseId\"\n      :school-course-name=\"schoolCourseName\"\n      @reload=\"getSchoolCourse\"\n      @close=\"closeDialogs\"\n    />\n  </div>\n</template>\n<script>\n  import api from '@/api/api'\n  import AddStudent from '@/components/schoolCourses/AddStudent.vue'\n  // import DeleteSchoolCourse from '@/components/schoolCourses/DeleteSchoolCourse.vue'\n  import EditSchoolCourse from '@/components/schoolCourses/EditSchoolCourse.vue'\n    export default {\n    name: 'EditSchoolCourses',\n\n    components: {\n      AddStudent,\n      // DeleteSchoolCourse,\n      EditSchoolCourse\n    },\n\n    layout: 'Default',\n      data: () => ({\n        dialog: false,\n        loading: false,\n        dialogEditSchoolCourse: false,\n        schoolCourseName: '',\n        schoolCourse: {}\n      }),\n      computed: {\n        schoolCourseId () {\n          return parseInt(this.$route.params.id)\n        }\n      },\n\n      async created () {\n        await this.getSchoolCourse()\n      },\n\n      methods: {\n        editSchoolCourse () {\n          this.schoolCourseName = this.schoolCourse.name\n          this.dialogEditSchoolCourse = true\n        },\n        closeDialogs () {\n          this.dialogEditSchoolCourse = false\n          this.schoolCourseName = ''\n        },\n        async deleteStudent(studentId) {\n          this.loading = true\n          try {\n            await api.unenrollment(studentId)\n            await this.getSchoolCourse()\n            this.loading = false\n          } catch (e) {\n            this.loading = false\n            console.log(e)\n          }\n        },\n\n        async getSchoolCourse () {\n          this.loading = true\n          try {\n            const request = await api.getSchoolCourse(this.schoolCourseId)\n            this.schoolCourse = request\n            this.loading = false\n          } catch (e) {\n            this.loading = false\n            console.log(e)\n          }\n        }\n      }\n  }\n</script>"],"mappings":"AAyFE,OAAOA,GAAE,MAAO,WAAU;AAC1B,OAAOC,UAAS,MAAO,2CAA0C;AACjE;AACA,OAAOC,gBAAe,MAAO,iDAAgD;AAC3E,eAAe;EACfC,IAAI,EAAE,mBAAmB;EAEzBC,UAAU,EAAE;IACVH,UAAU;IACV;IACAC;EACF,CAAC;EAEDG,MAAM,EAAE,SAAS;EACfC,IAAI,EAAEA,CAAA,MAAO;IACXC,MAAM,EAAE,KAAK;IACbC,OAAO,EAAE,KAAK;IACdC,sBAAsB,EAAE,KAAK;IAC7BC,gBAAgB,EAAE,EAAE;IACpBC,YAAY,EAAE,CAAC;EACjB,CAAC,CAAC;EACFC,QAAQ,EAAE;IACRC,cAAaA,CAAA,EAAK;MAChB,OAAOC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;IACvC;EACF,CAAC;EAED,MAAMC,OAAMA,CAAA,EAAK;IACf,MAAM,IAAI,CAACC,eAAe,CAAC;EAC7B,CAAC;EAEDC,OAAO,EAAE;IACPC,gBAAeA,CAAA,EAAK;MAClB,IAAI,CAACX,gBAAe,GAAI,IAAI,CAACC,YAAY,CAACR,IAAG;MAC7C,IAAI,CAACM,sBAAqB,GAAI,IAAG;IACnC,CAAC;IACDa,YAAWA,CAAA,EAAK;MACd,IAAI,CAACb,sBAAqB,GAAI,KAAI;MAClC,IAAI,CAACC,gBAAe,GAAI,EAAC;IAC3B,CAAC;IACD,MAAMa,aAAaA,CAACC,SAAS,EAAE;MAC7B,IAAI,CAAChB,OAAM,GAAI,IAAG;MAClB,IAAI;QACF,MAAMR,GAAG,CAACyB,YAAY,CAACD,SAAS;QAChC,MAAM,IAAI,CAACL,eAAe,CAAC;QAC3B,IAAI,CAACX,OAAM,GAAI,KAAI;MACrB,EAAE,OAAOkB,CAAC,EAAE;QACV,IAAI,CAAClB,OAAM,GAAI,KAAI;QACnBmB,OAAO,CAACC,GAAG,CAACF,CAAC;MACf;IACF,CAAC;IAED,MAAMP,eAAcA,CAAA,EAAK;MACvB,IAAI,CAACX,OAAM,GAAI,IAAG;MAClB,IAAI;QACF,MAAMqB,OAAM,GAAI,MAAM7B,GAAG,CAACmB,eAAe,CAAC,IAAI,CAACN,cAAc;QAC7D,IAAI,CAACF,YAAW,GAAIkB,OAAM;QAC1B,IAAI,CAACrB,OAAM,GAAI,KAAI;MAC